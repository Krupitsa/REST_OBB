{
	"info": {
		"_postman_id": "73927b54-2c73-4734-a8ee-88e4efbc27dc",
		"name": "Token session",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "!Token session termination",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 204\", function () {\r",
							"    pm.response.to.have.status(204);\r",
							"});\r",
							"\r",
							"pm.test(\"Body is correct\", function () {\r",
							"    pm.response.to.have.body(\"\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
							"value": "{{token}}",
							"type": "string"
						}
					]
				},
				"method": "DELETE",
				"header": [],
				"url": {
					"raw": "{{hostTest}}/authentication-web/api/auth",
					"host": [
						"{{hostTest}}"
					],
					"path": [
						"authentication-web",
						"api",
						"auth"
					]
				}
			},
			"response": []
		},
		{
			"name": "Getting the application with all dependencies",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 419\", function () {\r",
							"    pm.response.to.have.status(419);\r",
							"});\r",
							"\r",
							"pm.test(\"Version is correct\", function () {\r",
							"    pm.response.to.have.jsonBody(\"error\", \"The JWS token is not up to date.\");\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"auth": {
					"type": "bearer",
					"bearer": [
						{
							"key": "token",
							"value": "{{token}}",
							"type": "string"
						}
					]
				},
				"method": "GET",
				"header": [
					{
						"key": "accept",
						"value": "application/json",
						"type": "text"
					},
					{
						"key": "accept-charset",
						"value": "utf-8",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{hostTest}}/rest/app/Restapp/1.1.3",
					"host": [
						"{{hostTest}}"
					],
					"path": [
						"rest",
						"app",
						"Restapp",
						"1.1.3"
					]
				}
			},
			"response": []
		}
	]
}